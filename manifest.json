{
  "manifest_version": 2,
  "name": "Attention",
  "description": "Be attentive",
  "version": "0.1",
  "browser_action": {
    "default_icon": "specs.png",
    "default_popup":"popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "webRequest",
    "activeTab",
    "storage",
    "tabs",
    "http://*/*",
    "<all_urls>"
  ],
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }
]
}
